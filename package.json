{
  "name": "good-tests",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test:component": "vitest run ./src",
    "test:coverage": "vitest run --coverage",
    "test:application:vitest": "npx vitest run ./test/specs",
    "test:application:cypress": "npx cypress run",
    "test:application": "npm run test:application:cypress",
    "test:application:playwright": "npx playwright test"
  },
  "dependencies": {
    "@fastify/jwt": "^7.2.1",
    "@fastify/static": "^6.11.1",
    "better-sqlite3": "^8.6.0",
    "fastify": "^4.23.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.0",
    "@playwright/test": "^1.36.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^7.0.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.15",
    "cypress": "^13.2.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.1.0",
    "local-cypress": "^1.2.6",
    "msw": "^1.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.29",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.6.5",
    "webpack": "^5.88.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
